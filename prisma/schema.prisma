

generator client {
  provider = "prisma-client-js"
  // output = "../node_modules/@prisma/client" // Chemin par d√©faut, ne le mettez que si vous savez ce que vous faites
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model PM {
  humidity    Float
  pm10        Float
  pm25        Float
  pressure    Float
  temperature Float
  timestamp   DateTime @unique
}

model CO {
  lat       Float
  lon       Float
  CO        Float
  timestamp DateTime @unique
}
